<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="base.main_company" model="res.company">
            <field name="name">Військова частина А0000</field>
            <field name="partner_id" ref="base.main_partner"/>
            <field name="currency_id" ref="base.UAH"/>
            <field name="logo" type="base64" file="military_app/static/description/icon.png"/>
        </record>

        <record id="base.main_partner" model="res.partner">
            <field name="name">Військова частина А0000</field>
            <field name="company_name">Військова частина А0000</field>
            <field name="street">вул. Невідома 100</field>
            <field name="city">Краматорськ</field>
            <field name="zip">01234</field>
            <field name='country_id' ref='base.ua'/>
            <field name='state_id' ref='base.state_ua_05'/>
            <field name="phone">+380 99 123 456</field>
            <field name="email">info@a0000.com</field>
            <field name="website">www.a0000.com</field>
            <field name="image_1920" type="base64" file="military_app/static/description/icon.png"/>
        </record>

    </data>
